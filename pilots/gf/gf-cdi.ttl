ex:M a cdi:RepresentedVariable ;
cdi:name "Facility_ID" ;
cdi:displayLabel "Facility ID"@en , "Identifiant d'équipement"@fr ;
cdi:uses ex:A ;
cdi:definition "The ID of a variable containing a Facility"@en ;
